{
  "version": 1,
  "builds": [
    {
      "src": "technogyan/wsgi.py",
      "use": "@varcel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    },
    {
      "src": "build_files.sh",
      "use": "@varcel/static-build",
      "config": { "distDir": "staticfiles_build" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "technogyan/wsgi.py"
    }
  ]
}
